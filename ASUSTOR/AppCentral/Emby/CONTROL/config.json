{
 "general": {
   "package": "EmbyServer",
   "name": "Emby",
   "version": "Alpha 1.0",
   "depends": [ "docker-ce(>=20.10.2.r2)"],
   "developer": "Emby Media",
   "maintainer": "Emby-Media",
   "email": "support@Emby Media",
   "website": "https://emby.media",
   "architecture": "x86-64",
   "firmware": "2.0",
   "default-lang": "fr-FR",
   "memory-limit": "2048",
   "memory-advice": "2048",	 
   "privacy-statement": "https://emby.media/privacy.html/"},

   "adm-desktop":{
	       "app":{ 
		   "type":"custom",
		   "protocol":"http",
		   "port":"8096",
		   "url": "/" }
   		   },
	
   "privilege":{ 
	     "accessible": "administrators", 
	     "customizable": "true"
   	    },
   "register":{
   	"share-folder":[ {"name":"Docker", "description":"Default location for EmbyServer."} ],
   	"boot-priority":{ "start-order":65, "stop-order":35},
	"port":[8096]
   }
}
