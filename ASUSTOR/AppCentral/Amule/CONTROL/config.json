	          "conflicts":"",
	          "developer":"Amule",
	          "maintainer":"Drthrax74",
	          "email":"Drthrax74@gmail.com",
	          "website":"https://www.amule.org/",
	          "architecture":"x86-64",
	          "firmware":"4.1.0",
